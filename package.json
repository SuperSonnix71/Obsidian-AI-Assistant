{
    "name": "obsidian-ai-assistant",
    "version": "1.0.0",
    "description": "An Obsidian desktop plugin for AI interactions with Svelte",
    "main": "main.js",
    "scripts": {
        "dev": "tailwindcss -i ./src/styles.css -o ./styles.css --watch & node esbuild.config.mjs",
        "build": "tailwindcss -i ./src/styles.css -o ./styles.css && tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
        "version": "node version-bump.mjs && git add manifest.json versions.json",
        "svelte-check": "svelte-check --tsconfig tsconfig.json"
    },
    "keywords": [
        "obsidian",
        "obsidian-plugin",
        "ai",
        "llm"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@eslint/json": "^0.14.0",
        "@microsoft/eslint-plugin-sdl": "^1.1.0",
        "@types/node": "^16.11.6",
        "@types/nunjucks": "^3.2.6",
        "@typescript-eslint/eslint-plugin": "^7.18.0",
        "@typescript-eslint/parser": "^7.18.0",
        "autoprefixer": "^10.4.23",
        "builtin-modules": "3.3.0",
        "esbuild": "0.17.3",
        "esbuild-svelte": "^0.8.0",
        "eslint": "^8.57.1",
        "eslint-plugin-depend": "^1.4.0",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-obsidian": "^2.29.0",
        "eslint-plugin-obsidianmd": "^0.1.9",
        "globals": "^17.1.0",
        "obsidian": "latest",
        "postcss": "^8.5.6",
        "svelte": "^4.2.12",
        "svelte-check": "^3.6.2",
        "svelte-preprocess": "^5.1.3",
        "tailwindcss": "^3.4.17",
        "tslib": "2.4.0",
        "typescript": "~5.6.0",
        "typescript-eslint": "^8.53.1"
    },
    "dependencies": {
        "@types/uuid": "^10.0.0",
        "clsx": "^2.1.0",
        "nunjucks": "^3.2.4",
        "tailwind-merge": "^2.2.0",
        "uuid": "^13.0.0"
    }
}
